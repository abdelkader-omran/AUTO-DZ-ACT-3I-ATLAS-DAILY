{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "TRIZEL Ingest Records (JSONL) Schema",
  "type": "object",
  "required": [
    "target",
    "source_id",
    "requested_day_utc",
    "retrieved_utc",
    "url",
    "ok",
    "sha256",
    "bytes_len",
    "raw_path"
  ],
  "properties": {
    "target": { "type": "string" },
    "source_id": { "type": "string" },
    "requested_day_utc": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}$" },
    "retrieved_utc": { "type": "string" },
    "query": { "type": ["object", "null"] },
    "url": { "type": "string" },
    "ok": { "type": "boolean" },
    "status_code": { "type": ["integer", "null"] },
    "content_type": { "type": ["string", "null"] },
    "bytes_len": { "type": "integer", "minimum": 0 },
    "sha256": { "type": "string", "pattern": "^[a-f0-9]{64}$" },
    "raw_path": { "type": "string" },
    "error": { "type": ["string", "null"] },
    "license_hint": { "type": ["string", "null"] },
    "auth_required": { "type": ["boolean", "null"] }
  },
  "additionalProperties": true
}